    "/own-recipes/add": {
      "post": {
        "tags": ["Own recipes"],
        "summary": "Add own recipe",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [],
        "requestBody": {
          "description": "Add own recipe",
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/AddOwnRecipeRequest"
              }
            }
