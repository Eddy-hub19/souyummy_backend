        },
        "responses": {
          "200": {
            "description": "Successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AddOwnRecipeResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          },
          "409": {
            "description": "Recipe is already exist",
            "content": {}
          }
        }
      }
    },
    "/own-recipes/remove/{recipeId}":{
      "delete": {
        "tags": ["Own recipes"],
        "summary": "Delete own recipe",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "in": "path",
            "name": "recipeId",
            "required": true,
            "description": "Recipe id",
            "schema": {
              "type": "string"
            }
