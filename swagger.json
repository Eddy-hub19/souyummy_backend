    "/recipes/{id}": {
      "get": {
        "tags": ["Recipes"],
        "summary": "Recipe by id",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "description": "Recipe's id",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeByIdResponse"
                }
              }
            }
          },
          "404": {
            "description": "Recipe is not found.",
            "content": {}
          }
        }
      }
    },
